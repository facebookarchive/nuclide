{
  "name": "nuclide-analytics",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main",
  "version": "0.0.0",
  "description": "Provides a standard API for packages to send analytics to arbitrary providers.",
  "nuclide": {
    "customDeps": "lib/fb/package.json",
    "excludeTestsFromContinuousIntegration": true,
    "flowCheck": true,
    "packageType": "Node",
    "testRunner": "npm"
  },
  "dependencies": {
    "nuclide-commons": "0.0.0",
    "nuclide-logging": "0.0.0",
    "parse": "1.3.0"
  },
  "devDependencies": {
    "nuclide-external-interfaces": "0.0.0",
    "nuclide-jasmine": "0.0.0"
  },
  "scripts": {
    "test": "node --harmony node_modules/.bin/jasmine-node-transpiled spec"
  }
}
